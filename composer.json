{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nanuc/laravel-reolink"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-zip": "*",
        "chillerlan/php-qrcode": "^4.3",
        "chrisyue/php-m3u8": "^4",
        "dedoc/scramble": "^0.8.2",
        "doctrine/dbal": "^3.6",
        "guzzlehttp/guzzle": "^7.2",
        "jackiedo/dotenv-editor": "^2.1",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.14",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.12",
        "pbmedia/laravel-ffmpeg": "^8.1",
        "saloonphp/laravel-http-sender": "^1.2",
        "sammyjo20/saloon-laravel": "^2.0",
        "spatie/image": "^2.2",
        "spatie/laravel-data": "^3.1",
        "spatie/laravel-queueable-action": "^2.14",
        "spatie/laravel-ray": "^1.32",
        "spatie/laravel-tail": "^4.4",
        "symfony/yaml": "^6.3",
        "wireui/wireui": "^1.17"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^2.0",
        "ankitpokhrel/tus-php": "^2.3",
        "nanuc/laravel-reolink": "dev-master",
        "spatie/crypto": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
